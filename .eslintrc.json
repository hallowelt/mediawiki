{
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki",
		"plugin:vue/recommended"
	],
	"globals": {
		"require": "readonly",
		"module": "readonly",
		"OO": "readonly"
	},
	"rules": {
		"quote-props": [ "error", "as-needed" ],
		"max-len": "off",
		"no-jquery/no-global-selector": "off",
		"vue/html-indent": [ "warn", "tab" ]
	}
}
